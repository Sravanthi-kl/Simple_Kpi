<h4>Branch Kpi Details</h1>

<table class="table">
  <thead> 
        <th>Feed For </th>
        <th>Branch Name</th>
        <th>Kpi Category</th>
        <th>KPi Name</th>
        <th>KPI Frequency</th>        
  </thead>  

<% @mapped_kpi_ids.each do |mapped_kpi_id| %>
  <tr>   
    
      <td><%= Time.now.strftime("%m/%d/%Y") %></td>            
      <td><%= mapped_kpi_id.business.branchname%></td>
      <td><%= mapped_kpi_id.kpi.KPICategory%></td>
      <td><%= mapped_kpi_id.kpi.KPIName%></td>        
      <td><%= mapped_kpi_id.kpi.KPIEntryPeriod%></td>       
      <td><%= render :partial => 'plot1',:locals => {:kpi_id => mapped_kpi_id.kpi_id,:business_id => @business.id}%></td>      
  </tr>
<% end %>
	</tr>
</table>